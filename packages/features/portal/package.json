    "lowdb": "^1.0.0",
    "mitt": "^3.0.0",
    "mockjs": "^1.1.0",
    "moment": "^2.29.3",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.14",
    "portfinder": "^1.0.28",
    "query-string": "^7.1.1",
    "vite-svg-loader": "^3.3.0",
